TARGET= Hubbard_Ising.out
OBJS=  control_mod.o Operator.o observables_mod.o QDRP_decompose.o wrap_helpers.o \
       Hamiltonian_Hubbard_Ising.o Hop_mod.o  UDV_WRAP.o  Global_mod.o \
       tau_m.o   main.o wrapul.o  cgr1.o wrapgrup.o wrapur.o upgrade.o \
       wrapgrdo.o outconfc.o  inconfc.o  cgr2_2.o 


  
.SUFFIXES: .f90 .f
.f.o .f90.o:
	$(FC) -c $(F90USEFULFLAGS) -o $@ $(FLAGS) $<

$(TARGET): $(OBJS)
	$(FC) $(LF) -o $(TARGET) $(OBJS) $(LIBS)

clean:
	rm  $(OBJS)
  

