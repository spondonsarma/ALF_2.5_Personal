@article{Li15,
	Abstract = {Quantum critical phenomena may be qualitatively different when massless Dirac fermions are present at criticality. Using our recently-discovered fermion-sign-free Majorana quantum Monte Carlo method introduced by us in (Li et al 2015 Phys. Rev. B 91 [http://dx.doi.org/10.1103/physrevb.91.241117] 241117 ), we investigate the quantum critical phenomena of spinless Dirac fermions at their charge-density-wave phase transitions on the honeycomb lattice having ##IMG## [http://ej.iop.org/images/1367-2630/17/8/085003/njp514351ieqn1.gif] {${N}_{s}=2{L}^{2}$} sites with largest L = 24. By finite-size scaling, we accurately obtain critical exponents of this so-called Gross--Neveu chiral-Ising universality class of two (two-component) Dirac fermions in 2+1D: ##IMG## [http://ej.iop.org/images/1367-2630/17/8/085003/njp514351ieqn2.gif] {$\eta =0.45(2)$} , ##IMG## [http://ej.iop.org/images/1367-2630/17/8/085003/njp514351ieqn3.gif] {$\nu =0.77(3)$} , and ##IMG## [http://ej.iop.org/images/1367-2630/17/8/085003/njp514351ieqn4.gif] {$\beta =0.60(3)$} , which are qualitatively different from the mean-field results but are reasonably close to the ones obtained from renormalization group calculations.},
	Author = {Zi-Xiang Li and Yi-Fan Jiang and Hong Yao},
