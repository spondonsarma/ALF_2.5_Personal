	Issn = {0036-8075},
	Journal = {Science},
	Number = {6402},
	Pages = {570--574},
	Publisher = {American Association for the Advancement of Science},
	Title = {The role of electron-electron interactions in two-dimensional Dirac fermions},
	Url = {http://science.sciencemag.org/content/361/6402/570},
