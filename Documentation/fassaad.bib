	Issn = {0921-4534},
	Journal = {Physica C: Superconductivity and its Applications},
	Keywords = {Cuprates, ARPES, Strongly correlated electron systems, High temperature superconductors},
	Note = {Proceedings of the 8th International Conference on Materials and Mechanisms of Superconductivity and High Temperature Superconductors},
	Pages = {194 - 197},
	Title = {Universal waterfall-like feature in the spectral function of high temperature superconductors},
