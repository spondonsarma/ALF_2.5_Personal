	Date-Modified = {2016-02-23 09:00:06 +0000},
	Journal = {New Journal of Physics},
	Number = {8},
	Pages = {085003},
	Title = {Fermion-sign-free Majarana-quantum-Monte-Carlo studies of quantum critical phenomena of Dirac fermions in two dimensions},
