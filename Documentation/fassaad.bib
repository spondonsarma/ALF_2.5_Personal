@article{Affleck91,
	Abstract = {We investigate a class of isotropic SU(2n) quantum antiferromagnets for which we can find the exact ground states. These models contain nearest-neighbor interactions which are of higher order than the purely bilinear Heisenberg exchange interaction, and can be constructed for antisymmetric, self-conjugate representations of SU(2n) on bipartite lattices of coordination number z if n = mz (where m is an integer). The ground states are doubly degenerate, breaking both lattice-translation (t) and charge-conjugation (C) symmetries, and are emblematic of a new type of magnetic order (distinct from N{\'e}el and spin-Peierls order) unidentified in previous investigations of purely bilinear exchange models. We examine the general SU(4) nearest-neighbor spin chain with bilinear and biquadratic interactions in some detail and delineate the phase diagram. The C-breaking states are separated from dimerized states by a phase transition; a semiclassical mapping to a perturbed Wess-Zumino-Witten model enables us to study this transition. We find the expected integrable point at this transition directly from the Yang-Baxter equations. For two other choices of the ratio of the biquadratic to the bilinear coupling, the hamiltonian exhibits additional SU(6) symmetry and also corresponds to SU(2) spin-52 hamiltonians with fine-tuned parameters.},
	Author = {Ian Affleck and D.P. Arovas and J.B. Marston and D.A. Rabson},
