@article{Wang14a,
	Abstract = {Spinless fermions on a honeycomb lattice provide a minimal realization of lattice Dirac fermions. Repulsive interactions between nearest neighbors drive a quantum phase transition from a Dirac semimetal to a charge-density-wave state through a fermionic quantum critical point, where the coupling of the Ising order parameter to the Dirac fermions at low energy drastically affects the quantum critical behavior. Encouraged by a recent discovery (Huffman and Chandrasekharan 2014 Phys. Rev. B 89 [http://dx.doi.org/10.1103/PhysRevB.89.111101] 111101 ) of the absence of the fermion sign problem in this model, we study the fermionic quantum critical point using the continuous-time quantum Monte Carlo method with a worm-sampling technique. We estimate the transition point ##IMG## [http://ej.iop.org/images/1367-2630/16/10/103008/njp502274ieqn1.gif] {$V/t=1.356(1)$} with the critical exponents ##IMG## [http://ej.iop.org/images/1367-2630/16/10/103008/njp502274ieqn2.gif] {$\nu =0.80(3)$} and ##IMG## [http://ej.iop.org/images/1367-2630/16/10/103008/njp502274ieqn3.gif] {$\eta =0.302(7)$} . Compatible results for the transition point are also obtained with infinite projected entangled-pair states.},
	Author = {Lei Wang and Philippe Corboz and Matthias Troyer},
	Date-Added = {2015-03-11 09:20:53 +0000},
	Date-Modified = {2015-03-11 09:21:16 +0000},
	Journal = {New Journal of Physics},
	Number = {10},
	Pages = {103008},
	Title = {Fermionic quantum critical point of spinless fermions on a honeycomb lattice},
	Url = {http://stacks.iop.org/1367-2630/16/i=10/a=103008},
	Volume = {16},
	Year = {2014},
	Bdsk-Url-1 = {http://stacks.iop.org/1367-2630/16/i=10/a=103008}}

