@article{doenni96,
	Abstract = {The heavy-fermion compound CePdAl with ZrNiAl-type crystal structure (hexagonal space group ##IMG## [http://ej.iop.org/images/0953-8984/8/50/043/img8.gif] ) was investigated by powder neutron diffraction. The triangular coordination symmetry of magnetic Ce atoms on site 3f gives rise to geometrical frustration. CePdAl orders below ##IMG## [http://ej.iop.org/images/0953-8984/8/50/043/img9.gif] with an incommensurate antiferromagnetic propagation vector ##IMG## [http://ej.iop.org/images/0953-8984/8/50/043/img10.gif] , and a longitudinal sine-wave (LSW) modulated spin arrangement. Magnetically ordered moments at Ce(1) and Ce(3) coexist with frustrated disordered moments at Ce(2). The experimentally determined magnetic structure is in agreement with group theoretical symmetry analysis considerations, calculated by the program MODY, which confirm that for Ce(2) an ordered magnetic moment parallel to the magnetically easy c-axis is forbidden by symmetry. Further low-temperature experiments give evidence for a second magnetic phase transition in CePdAl between 0.6 and 1.3 K. Magnetic structures of CePdAl are compared with those of the isostructural compound TbNiAl, where a non-zero ordered magnetic moment for the geometrically frustrated Tb(2) atoms is allowed by symmetry.},
	Author = {A D\"onni and G Ehlers and H Maletta and P Fischer and H Kitazawa and M Zolliker},
